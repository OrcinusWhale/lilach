<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.CheckoutController">
   <padding>
      <Insets bottom="40.0" left="40.0" right="40.0" top="40.0" />
   </padding>
   <children>
      <Label text="Checkout Working Test" textFill="#2E8B57">
         <font>
            <Font name="System Bold" size="28.0" />
         </font>
      </Label>
      <Label text="Test checkout page" />
      <Button fx:id="backToCartButton" mnemonicParsing="false" onAction="#goBackToCart" text="Back to Cart" />
   </children>
</VBox>
